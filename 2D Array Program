import java.util.*;
public class Array2D2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num[][]=new int[3][3];
		System.out.println("Enter numbers in array:");
		for(int i=0;i<3;i++){
			for(int j=0;j<3;j++) {
			  num[i][j]=sc.nextInt();
		}
	  }
		System.out.println("Resultant Matrix:");
		for(int i=0;i<3;i++){
			for(int j=0;j<3;j++) {
			  System.out.print(" ");
			  System.out.print(num[i][j]);
		}
			System.out.println();
	  }

  }
}
